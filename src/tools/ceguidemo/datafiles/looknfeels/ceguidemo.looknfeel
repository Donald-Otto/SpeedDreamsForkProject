        <ImagerySection name="ascend_icon">
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge" ><AbsoluteDim value="3" /></Dim>
                    <Dim type="TopEdge" ><AbsoluteDim value="3" /></Dim>
                    <Dim type="Width" ><ImageDim imageset="CEGUIDemo" image="HeaderBarSortUp" dimension="Width" /></Dim>
                    <Dim type="Height" ><ImageDim imageset="CEGUIDemo" image="HeaderBarSortUp" dimension="Height" /></Dim>
                </Area>
                <Image imageset="CEGUIDemo" image="HeaderBarSortDown" />

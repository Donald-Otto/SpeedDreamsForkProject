<GUILayout>
    <Window Name="MenuResults" Type="CEGUIDemo/StaticImage">
        <Property Name="UnifiedAreaRect" Value="{{0,0},{0,0},{1,0},{1,0}}" />
        <Property Name="Image" Value="set:SplashCredits image:everything" />
        <Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}" />
        <Property Name="FrameEnabled" Value="False" />
        <Property Name="BackgroundEnabled" Value="False" />
        <Window Name="MenuResults/ImgLogo" Type="CEGUIDemo/StaticImage">
            <Property Name="UnifiedAreaRect" Value="{{0,0},{0,0},{0.3,0},{0.25,0}}" />
            <Property Name="Image" Value="set:Logo image:everything" />
            <Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}" />
            <Property Name="FrameEnabled" Value="False" />
            <Property Name="BackgroundEnabled" Value="False" />
            <Property Name="Tooltip" Value="What a nice logo !" />
        </Window>
        <Window Name="MenuResults/TxtTitle" Type="CEGUIDemo/PageTitle">
            <Property Name="UnifiedAreaRect" Value="{{0.22,0},{0.1,0},{0.92,0},{0.2,0}}" />
            <Property Name="Font" Value="MenuTitle" />
            <Property Name="Text" Value="Race results" />
            <Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}" />
            <Property Name="Tooltip" Value="Yeah, this is the title of the menu !" />
            <Property Name="FrameEnabled" Value="False" />
        </Window>
        <Window Name="MenuResults/TxtFrameRate" Type="CEGUIDemo/StaticText">
            <Property Name="UnifiedAreaRect" Value="{{0.95,0},{0.01,0},{0.99,0},{0.04,0}}" />
            <Property Name="Font" Value="TextSmall" />
            <Property Name="Text" Value="99.9" />
            <Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}" />
            <Property Name="TextColours" Value="tl:FFFFFFFF tr:FFC0C0C0 bl:FF808080 br:FF606060" />
            <Property Name="Tooltip" Value="Frame rate (F/s)" />
            <Property Name="FrameEnabled" Value="False" />
            <Property Name="HorzFormatting" Value="RightAligned" />
            <Property Name="VertFormatting" Value="TopAligned" />
            <Property Name="BackgroundEnabled" Value="False" />
        </Window>
        <Window Name="MenuResults/MclTable" Type="CEGUIDemo/MultiColumnList">
            <Property Name="UnifiedAreaRect" Value="{{0.05,0},{0.25,0},{0.95,0},{0.85,0}}" />
            <Property Name="HeaderFont" Value="MenuSmall" />
            <Property Name="Font" Value="TextNormal" />
            <Property Name="Tooltip" Value="Detailled race results" />
            <Property Name="ColumnsMovable" Value="True" />
            <Property Name="ColumnsSizable" Value="True" />
            <Property Name="SortSettingEnabled" Value="True" />
        </Window>
        <Window Name="MenuResults/BtnContinue" Type="CEGUIDemo/Button">
            <Property Name="UnifiedAreaRect" Value="{{0.43,0},{0.9,0},{0.58,0},{0.95,0}}" />
            <Property Name="Font" Value="MenuMedium" />
            <Property Name="Text" Value="Continue" />
            <Property Name="Tooltip" Value="Go on with next session / race if any" />
        </Window>
    </Window>
</GUILayout>
